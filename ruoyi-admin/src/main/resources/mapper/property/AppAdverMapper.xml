<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.aylives.ruoyi.admin.property.mapper.AppAdverMapper">
    
    <resultMap type="AppAdver" id="AppAdverResult">
        <result property="id"    column="id"    />
        <result property="name"    column="name"    />
        <result property="link"    column="link"    />
        <result property="type"    column="type"    />
        <result property="sort"    column="sort"    />
        <result property="beginTime"    column="begin_time"    />
        <result property="endTime"    column="end_time"    />
        <result property="createdDate"    column="created_date"    />
        <result property="imgUrl"    column="img_url"    />
        <result property="status"    column="status"    />
    </resultMap>

    <sql id="selectAppAdverVo">
        select id, name, link, type, sort, begin_time, end_time, created_date, img_url, status from aomygod.app_adver
    </sql>

    <select id="selectAppAdverList" parameterType="AppAdver" resultMap="AppAdverResult">
        <include refid="selectAppAdverVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name = #{name}</if>
            <if test="link != null  and link != ''"> and link = #{link}</if>
            <if test="type != null "> and type = #{type}</if>
            <if test="sort != null "> and sort = #{sort}</if>
            <if test="beginTime != null "> and begin_time = #{beginTime}</if>
            <if test="endTime != null "> and end_time = #{endTime}</if>
            <if test="createdDate != null "> and created_date = #{createdDate}</if>
            <if test="imgUrl != null  and imgUrl != ''"> and img_url = #{imgUrl}</if>
            <if test="status != null "> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectAppAdverById" parameterType="Long" resultMap="AppAdverResult">
        <include refid="selectAppAdverVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertAppAdver" parameterType="AppAdver" useGeneratedKeys="true" keyProperty="id">
        insert into aomygod.app_adver
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null  and name != ''">name,</if>
            <if test="link != null  and link != ''">link,</if>
            <if test="type != null ">type,</if>
            <if test="sort != null ">sort,</if>
            <if test="beginTime != null ">begin_time,</if>
            <if test="endTime != null ">end_time,</if>
            <if test="createdDate != null ">created_date,</if>
            <if test="imgUrl != null  and imgUrl != ''">img_url,</if>
            <if test="status != null ">status,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null  and name != ''">#{name},</if>
            <if test="link != null  and link != ''">#{link},</if>
            <if test="type != null ">#{type},</if>
            <if test="sort != null ">#{sort},</if>
            <if test="beginTime != null ">#{beginTime},</if>
            <if test="endTime != null ">#{endTime},</if>
            <if test="createdDate != null ">#{createdDate},</if>
            <if test="imgUrl != null  and imgUrl != ''">#{imgUrl},</if>
            <if test="status != null ">#{status},</if>
         </trim>
    </insert>

    <update id="updateAppAdver" parameterType="AppAdver">
        update aomygod.app_adver
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null  and name != ''">name = #{name},</if>
            <if test="link != null  and link != ''">link = #{link},</if>
            <if test="type != null ">type = #{type},</if>
            <if test="sort != null ">sort = #{sort},</if>
            <if test="beginTime != null ">begin_time = #{beginTime},</if>
            <if test="endTime != null ">end_time = #{endTime},</if>
            <if test="createdDate != null ">created_date = #{createdDate},</if>
            <if test="imgUrl != null  and imgUrl != ''">img_url = #{imgUrl},</if>
            <if test="status != null ">status = #{status},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteAppAdverById" parameterType="Long">
        delete from aomygod.app_adver where id = #{id}
    </delete>

    <delete id="deleteAppAdverByIds" parameterType="String">
        delete from aomygod.app_adver where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
</mapper>