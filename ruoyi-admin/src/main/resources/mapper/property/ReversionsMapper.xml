<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.aylives.ruoyi.admin.property.mapper.ReversionsMapper">
    
    <resultMap type="Reversions" id="ReversionsResult">
        <result property="id"    column="id"    />
        <result property="agencyId"    column="agency_id"    />
        <result property="replyType"    column="reply_type"    />
        <result property="classify"    column="classify"    />
        <result property="keyword"    column="keyword"    />
        <result property="replyContent"    column="reply_content"    />
        <result property="status"    column="status"    />
        <result property="createdTime"    column="created_time"    />
        <result property="updatedTime"    column="updated_time"    />
    </resultMap>

    <sql id="selectReversionsVo">
        select id, agency_id, reply_type, classify, keyword, reply_content, status, created_time, updated_time from aomygod.reversions
    </sql>

    <select id="selectReversionsList" parameterType="Reversions" resultMap="ReversionsResult">
        <include refid="selectReversionsVo"/>
        <where>  
            <if test="agencyId != null "> and agency_id = #{agencyId}</if>
            <if test="replyType != null  and replyType != ''"> and reply_type = #{replyType}</if>
            <if test="classify != null  and classify != ''"> and classify = #{classify}</if>
            <if test="keyword != null  and keyword != ''"> and keyword = #{keyword}</if>
            <if test="replyContent != null  and replyContent != ''"> and reply_content = #{replyContent}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="createdTime != null "> and created_time = #{createdTime}</if>
            <if test="updatedTime != null "> and updated_time = #{updatedTime}</if>
        </where>
    </select>
    
    <select id="selectReversionsById" parameterType="Integer" resultMap="ReversionsResult">
        <include refid="selectReversionsVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertReversions" parameterType="Reversions" useGeneratedKeys="true" keyProperty="id">
        insert into aomygod.reversions
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="agencyId != null ">agency_id,</if>
            <if test="replyType != null  and replyType != ''">reply_type,</if>
            <if test="classify != null  and classify != ''">classify,</if>
            <if test="keyword != null  and keyword != ''">keyword,</if>
            <if test="replyContent != null  and replyContent != ''">reply_content,</if>
            <if test="status != null  and status != ''">status,</if>
            <if test="createdTime != null ">created_time,</if>
            <if test="updatedTime != null ">updated_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="agencyId != null ">#{agencyId},</if>
            <if test="replyType != null  and replyType != ''">#{replyType},</if>
            <if test="classify != null  and classify != ''">#{classify},</if>
            <if test="keyword != null  and keyword != ''">#{keyword},</if>
            <if test="replyContent != null  and replyContent != ''">#{replyContent},</if>
            <if test="status != null  and status != ''">#{status},</if>
            <if test="createdTime != null ">#{createdTime},</if>
            <if test="updatedTime != null ">#{updatedTime},</if>
         </trim>
    </insert>

    <update id="updateReversions" parameterType="Reversions">
        update aomygod.reversions
        <trim prefix="SET" suffixOverrides=",">
            <if test="agencyId != null ">agency_id = #{agencyId},</if>
            <if test="replyType != null  and replyType != ''">reply_type = #{replyType},</if>
            <if test="classify != null  and classify != ''">classify = #{classify},</if>
            <if test="keyword != null  and keyword != ''">keyword = #{keyword},</if>
            <if test="replyContent != null  and replyContent != ''">reply_content = #{replyContent},</if>
            <if test="status != null  and status != ''">status = #{status},</if>
            <if test="createdTime != null ">created_time = #{createdTime},</if>
            <if test="updatedTime != null ">updated_time = #{updatedTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteReversionsById" parameterType="Integer">
        delete from aomygod.reversions where id = #{id}
    </delete>

    <delete id="deleteReversionsByIds" parameterType="String">
        delete from aomygod.reversions where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
</mapper>