<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.aylives.ruoyi.admin.property.mapper.AppUserMapper">
    
    <resultMap type="AppUser" id="AppUserResult">
        <result property="userId"    column="USER_ID"    />
        <result property="username"    column="USERNAME"    />
        <result property="password"    column="PASSWORD"    />
        <result property="name"    column="NAME"    />
        <result property="rights"    column="RIGHTS"    />
        <result property="roleId"    column="ROLE_ID"    />
        <result property="lastLogin"    column="LAST_LOGIN"    />
        <result property="ip"    column="IP"    />
        <result property="status"    column="STATUS"    />
        <result property="bz"    column="BZ"    />
        <result property="phone"    column="PHONE"    />
        <result property="sfid"    column="SFID"    />
        <result property="startTime"    column="START_TIME"    />
        <result property="endTime"    column="END_TIME"    />
        <result property="years"    column="YEARS"    />
        <result property="number"    column="NUMBER"    />
        <result property="email"    column="EMAIL"    />
        <result property="source"    column="SOURCE"    />
        <result property="birthday"    column="BIRTHDAY"    />
        <result property="sex"    column="SEX"    />
        <result property="avatar"    column="AVATAR"    />
        <result property="version"    column="VERSION"    />
        <result property="appVersion"    column="APP_VERSION"    />
        <result property="deviceId"    column="DEVICE_ID"    />
        <result property="nickname"    column="NICKNAME"    />
        <result property="tag"    column="TAG"    />
        <result property="regAgencyId"    column="REG_AGENCY_ID"    />
        <result property="loginCount"    column="login_count"    />
        <result property="origin"    column="origin"    />
    </resultMap>

    <sql id="selectAppUserVo">
        select USER_ID, USERNAME, PASSWORD, NAME, RIGHTS, ROLE_ID, LAST_LOGIN, IP, STATUS, BZ, PHONE, SFID, START_TIME, END_TIME, YEARS, NUMBER, EMAIL, SOURCE, BIRTHDAY, SEX, AVATAR, VERSION, APP_VERSION, DEVICE_ID, NICKNAME, TAG, REG_AGENCY_ID, login_count, origin from aomygod.sys_app_user
    </sql>

    <select id="selectAppUserList" parameterType="AppUser" resultMap="AppUserResult">
        <include refid="selectAppUserVo"/>
        <where>  
            <if test="phone != null  and phone != ''"> and PHONE = #{phone}</if>
            <if test="nickname != null  and nickname != ''"> and NICKNAME = #{nickname}</if>
            <if test="loginCount != null "> and login_count = #{loginCount}</if>
        </where>
    </select>
    
    <select id="selectAppUserById" parameterType="Long" resultMap="AppUserResult">
        <include refid="selectAppUserVo"/>
        where USER_ID = #{userId}
    </select>

    <select id="selectAppUserByUserIds" resultMap="AppUserResult" >
        <include refid="selectAppUserVo"/>    where USER_ID in
        <foreach item="userId" collection="list" open="(" separator="," close=")">
            #{userId}
        </foreach>
    </select>

    <insert id="insertAppUser" parameterType="AppUser" useGeneratedKeys="true" keyProperty="userId">
        insert into aomygod.sys_app_user
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="username != null  and username != ''">USERNAME,</if>
            <if test="password != null  and password != ''">PASSWORD,</if>
            <if test="name != null  and name != ''">NAME,</if>
            <if test="rights != null  and rights != ''">RIGHTS,</if>
            <if test="roleId != null  and roleId != ''">ROLE_ID,</if>
            <if test="lastLogin != null  and lastLogin != ''">LAST_LOGIN,</if>
            <if test="ip != null  and ip != ''">IP,</if>
            <if test="status != null  and status != ''">STATUS,</if>
            <if test="bz != null  and bz != ''">BZ,</if>
            <if test="phone != null  and phone != ''">PHONE,</if>
            <if test="sfid != null  and sfid != ''">SFID,</if>
            <if test="startTime != null  and startTime != ''">START_TIME,</if>
            <if test="endTime != null  and endTime != ''">END_TIME,</if>
            <if test="years != null ">YEARS,</if>
            <if test="number != null  and number != ''">NUMBER,</if>
            <if test="email != null  and email != ''">EMAIL,</if>
            <if test="source != null  and source != ''">SOURCE,</if>
            <if test="birthday != null  and birthday != ''">BIRTHDAY,</if>
            <if test="sex != null ">SEX,</if>
            <if test="avatar != null  and avatar != ''">AVATAR,</if>
            <if test="version != null  and version != ''">VERSION,</if>
            <if test="appVersion != null  and appVersion != ''">APP_VERSION,</if>
            <if test="deviceId != null  and deviceId != ''">DEVICE_ID,</if>
            <if test="nickname != null  and nickname != ''">NICKNAME,</if>
            <if test="tag != null  and tag != ''">TAG,</if>
            <if test="regAgencyId != null ">REG_AGENCY_ID,</if>
            <if test="loginCount != null ">login_count,</if>
            <if test="origin != null  and origin != ''">origin,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="username != null  and username != ''">#{username},</if>
            <if test="password != null  and password != ''">#{password},</if>
            <if test="name != null  and name != ''">#{name},</if>
            <if test="rights != null  and rights != ''">#{rights},</if>
            <if test="roleId != null  and roleId != ''">#{roleId},</if>
            <if test="lastLogin != null  and lastLogin != ''">#{lastLogin},</if>
            <if test="ip != null  and ip != ''">#{ip},</if>
            <if test="status != null  and status != ''">#{status},</if>
            <if test="bz != null  and bz != ''">#{bz},</if>
            <if test="phone != null  and phone != ''">#{phone},</if>
            <if test="sfid != null  and sfid != ''">#{sfid},</if>
            <if test="startTime != null  and startTime != ''">#{startTime},</if>
            <if test="endTime != null  and endTime != ''">#{endTime},</if>
            <if test="years != null ">#{years},</if>
            <if test="number != null  and number != ''">#{number},</if>
            <if test="email != null  and email != ''">#{email},</if>
            <if test="source != null  and source != ''">#{source},</if>
            <if test="birthday != null  and birthday != ''">#{birthday},</if>
            <if test="sex != null ">#{sex},</if>
            <if test="avatar != null  and avatar != ''">#{avatar},</if>
            <if test="version != null  and version != ''">#{version},</if>
            <if test="appVersion != null  and appVersion != ''">#{appVersion},</if>
            <if test="deviceId != null  and deviceId != ''">#{deviceId},</if>
            <if test="nickname != null  and nickname != ''">#{nickname},</if>
            <if test="tag != null  and tag != ''">#{tag},</if>
            <if test="regAgencyId != null ">#{regAgencyId},</if>
            <if test="loginCount != null ">#{loginCount},</if>
            <if test="origin != null  and origin != ''">#{origin},</if>
         </trim>
    </insert>

    <update id="updateAppUser" parameterType="AppUser">
        update aomygod.sys_app_user
        <trim prefix="SET" suffixOverrides=",">
            <if test="username != null  and username != ''">USERNAME = #{username},</if>
            <if test="password != null  and password != ''">PASSWORD = #{password},</if>
            <if test="name != null  and name != ''">NAME = #{name},</if>
            <if test="rights != null  and rights != ''">RIGHTS = #{rights},</if>
            <if test="roleId != null  and roleId != ''">ROLE_ID = #{roleId},</if>
            <if test="lastLogin != null  and lastLogin != ''">LAST_LOGIN = #{lastLogin},</if>
            <if test="ip != null  and ip != ''">IP = #{ip},</if>
            <if test="status != null  and status != ''">STATUS = #{status},</if>
            <if test="bz != null  and bz != ''">BZ = #{bz},</if>
            <if test="phone != null  and phone != ''">PHONE = #{phone},</if>
            <if test="sfid != null  and sfid != ''">SFID = #{sfid},</if>
            <if test="startTime != null  and startTime != ''">START_TIME = #{startTime},</if>
            <if test="endTime != null  and endTime != ''">END_TIME = #{endTime},</if>
            <if test="years != null ">YEARS = #{years},</if>
            <if test="number != null  and number != ''">NUMBER = #{number},</if>
            <if test="email != null  and email != ''">EMAIL = #{email},</if>
            <if test="source != null  and source != ''">SOURCE = #{source},</if>
            <if test="birthday != null  and birthday != ''">BIRTHDAY = #{birthday},</if>
            <if test="sex != null ">SEX = #{sex},</if>
            <if test="avatar != null  and avatar != ''">AVATAR = #{avatar},</if>
            <if test="version != null  and version != ''">VERSION = #{version},</if>
            <if test="appVersion != null  and appVersion != ''">APP_VERSION = #{appVersion},</if>
            <if test="deviceId != null  and deviceId != ''">DEVICE_ID = #{deviceId},</if>
            <if test="nickname != null  and nickname != ''">NICKNAME = #{nickname},</if>
            <if test="tag != null  and tag != ''">TAG = #{tag},</if>
            <if test="regAgencyId != null ">REG_AGENCY_ID = #{regAgencyId},</if>
            <if test="loginCount != null ">login_count = #{loginCount},</if>
            <if test="origin != null  and origin != ''">origin = #{origin},</if>
        </trim>
        where USER_ID = #{userId}
    </update>

    <delete id="deleteAppUserById" parameterType="Long">
        delete from aomygod.sys_app_user where USER_ID = #{userId}
    </delete>

    <delete id="deleteAppUserByIds" parameterType="String">
        delete from aomygod.sys_app_user where USER_ID in
